    <url-pattern>/GoShow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Goods</servlet-name>
    <url-pattern>/Goods</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
    <filter>
        <filter-name>fiter</filter-name>
        <filter-class>com.fiter.doFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>fiter</filter-name>
        <url-pattern>/order.jsp</url-pattern>
        <url-pattern>/orders.jsp</url-pattern>
        <url-pattern>/showGood.jsp</url-pattern>
    </filter-mapping>
