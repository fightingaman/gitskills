    <servlet-class>com.Severlet.Goods</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>BackServlet</servlet-name>
    <url-pattern>/BackServlet</url-pattern>
  </servlet-mapping>



  <servlet-mapping>
    <servlet-name>GoodShow</servlet-name>
    <url-pattern>/GoodShow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyCart</servlet-name>
    <url-pattern>/MyCart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Sall</servlet-name>
    <url-pattern>/Sall</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Order</servlet-name>
    <url-pattern>/Order</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Userset</servlet-name>
    <url-pattern>/Userset</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ajaxLogin</servlet-name>
    <url-pattern>/ajaxLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
