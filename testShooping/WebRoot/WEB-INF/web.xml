    <servlet-name>Sall</servlet-name>
    <servlet-class>com.Severlet.Sall</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Order</servlet-name>
    <servlet-class>com.Severlet.Order</servlet-class>
  </servlet>

<servlet>
    <description>支付结果</description>
    <display-name>支付结果</display-name>
    <servlet-name>BackServlet</servlet-name>
    <servlet-class>com.Severlet.BackServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Userset</servlet-name>
    <servlet-class>com.Severlet.Userset</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ajaxLogin</servlet-name>
    <servlet-class>com.Severlet.ajaxLogin</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GoShow</servlet-name>
    <servlet-class>com.Severlet.GoShow</servlet-class>
  </servlet>
  <servlet>
